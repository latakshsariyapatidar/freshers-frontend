{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "http://ec2-51-21-192-129.eu-north-1.compute.amazonaws.com:5000/api/$1"
    },
    {
      "source": "/socket.io/(.*)",
      "destination": "http://ec2-51-21-192-129.eu-north-1.compute.amazonaws.com:5000/socket.io/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
